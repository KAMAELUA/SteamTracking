	layout
	{
		region	{ name="bottom" width="1262" y=75 height=60 width=400 }
		
		region { name="column1" x=0 width=184 }
		region { name="column2" x=210 width=290 }
		region { name="column3" x=500 width=max }
		region { name="column4" x=700 width=max overflow=allow-horizontal }
		
		place	{ control="bgimage1" width=max height="max" }

		// left column
		place { control="gameimage" region="column1" margin-left=20 margin-top=20 width=max height=69 }
		
