	{

		links
		{
			font-size=14
			font-style=normal
			textcolor="text"
			font-family=basefont
			font-style="uppercase"
			padding-left=0
			padding-top=0
			padding-bottom=0
		}
		
		links:hover
		{
			font-size=14
			font-style=normal
			textcolor="white"
			font-family=basefont
			font-style="uppercase"
